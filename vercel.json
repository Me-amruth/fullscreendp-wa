{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "index.js" },
    { "src": "/connect", "dest": "routes/connection.js" },
    { "src": "/clear", "dest": "routes/clear.js" },
    { "src": "/upload", "dest": "routes/uploader.js" }
  ]
}
